<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <id value="Profile-Ionisiertes-Kalzium-Nierenersatzverfahren" />
  <url value="https://medizininformatik-initiative.de/fhir/ext/modul-icu/StructureDefinition/Ionisiertes-Kalzium-Nierenersatzverfahren" />
  <name value="MII_Ionisiertes_Kalzium_Nierenersatzverfahren" />
  <status value="active" />
  <fhirVersion value="4.0.1" />
  <kind value="resource" />
  <abstract value="false" />
  <type value="Observation" />
  <baseDefinition value="https://medizininformatik-initiative.de/fhir/ext/modul-icu/StructureDefinition/Parameter-Von-Extrakorporalen-Verfahren" />
  <derivation value="constraint" />
  <differential>
    <element id="Observation.category">
      <path value="Observation.category" />
      <min value="1" />
    </element>
    <element id="Observation.code">
      <path value="Observation.code" />
      <constraint>
        <key value="obs-loinc-sct" />
        <severity value="error" />
        <human value="Es muss mindestens ein LOINC oder SnomedCT Code vorhanden sein" />
        <expression value="coding.where(system='http://loinc.org').exists().not() implies coding.where(system='http://snomed.info/sct').exists()" />
        <source value="https://medizininformatik-initiative.de/fhir/ext/modul-icu/StructureDefinition/Ionisiertes-Kalzium-Nierenersatzverfahren" />
      </constraint>
    </element>
    <element id="Observation.code.coding:loinc">
      <path value="Observation.code.coding" />
      <sliceName value="loinc" />
      <patternCoding>
        <code value="83064-6" />
      </patternCoding>
    </element>
    <element id="Observation.effective[x]">
      <path value="Observation.effective[x]" />
      <min value="1" />
    </element>
    <element id="Observation.value[x]">
      <path value="Observation.value[x]" />
      <patternQuantity>
		<system value="http://unitsofmeasure.org" />
        <code value="mmol/L" />
      </patternQuantity>
    </element>
  </differential>
</StructureDefinition>