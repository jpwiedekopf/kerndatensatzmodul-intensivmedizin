<?xml version='1.0' encoding='utf-8'?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <id value="sd-mii-icu-ionisiertes-kalzium-nierenersatzverfahren" />
  <url value="https://www.medizininformatik-initiative.de/fhir/ext/modul-icu/StructureDefinition/ionisiertes-kalzium-nierenersatzverfahren" />
  <version value="1.0.0" />
  <name value="SD_MII_ICU_Ionisiertes_Kalzium_Nierenersatzverfahren" />
  <title value="SD MII ICU Ionisiertes Kalzium Nierenersatzverfahren" />
  <status value="active" />
  <fhirVersion value="4.0.1" />
  <kind value="resource" />
  <abstract value="false" />
  <type value="Observation" />
  <baseDefinition value="https://www.medizininformatik-initiative.de/fhir/ext/modul-icu/StructureDefinition/parameter-von-extrakorporalen-verfahren" />
  <derivation value="constraint" />
  <differential>
    <element id="Observation.category">
      <path value="Observation.category" />
      <min value="1" />
    </element>
    <element id="Observation.code">
      <path value="Observation.code" />
      <constraint>
        <key value="obs-loinc-sct" />
        <severity value="error" />
        <human value="Es muss mindestens ein LOINC oder SnomedCT Code vorhanden sein" />
        <expression value="coding.where(system='http://loinc.org').exists().not() implies coding.where(system='http://snomed.info/sct').exists()" />
        <source value="https://www.medizininformatik-initiative.de/fhir/ext/modul-icu/StructureDefinition/ionisiertes-kalzium-nierenersatzverfahren" />
      </constraint>
    </element>
    <element id="Observation.code.coding:loinc">
      <path value="Observation.code.coding" />
      <sliceName value="loinc" />
      <patternCoding>
        <system value="http://loinc.org" />
        <code value="83064-6" />
      </patternCoding>
    </element>
    <element id="Observation.effective[x]">
      <path value="Observation.effective[x]" />
      <min value="1" />
    </element>
    <element id="Observation.value[x]">
      <path value="Observation.value[x]" />
      <patternQuantity>
        <system value="http://unitsofmeasure.org" />
        <code value="mmol/L" />
      </patternQuantity>
    </element>
  </differential>
</StructureDefinition>